{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "pythonArgs": [
                "-Xfrozen_modules=off",
            ]
        }
    ]
}